[HKEY_CLASSES_ROOT\*\shell\ftpUploader]
@="ftpUploader"

[HKEY_CLASSES_ROOT\*\shell\ftpUploader\command]
@="{currentfolder}\\ftpUploader.exe \"%1\""


[HKEY_CLASSES_ROOT\Directory\Background\shell\ftpUploader]
@="ftpUploader"

[HKEY_CLASSES_ROOT\Directory\Background\shell\ftpUploader\command]
@="{currentfolder}\\ftpUploader.exe"

